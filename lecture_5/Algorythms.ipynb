    "_end = '_end_'\n",
    "\n",
    " \n",
    "def make_trie(*words):\n",
    "    root = dict()\n",
    "    for word in words:\n",
    "        current_dict = root\n",
    "        for letter in word:\n",
    "            current_dict = current_dict.setdefault(letter, {})\n",
    "        current_dict[_end] = _end\n",
    "    return root\n",
    "\n",
    "make_trie('foo', 'bar', 'baz', 'barz')\n"
